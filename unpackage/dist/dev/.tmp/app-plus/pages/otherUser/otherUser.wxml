view></view><block wx:if="{{items.$orig.content!=''}}"><view class="content"><block wx:if="{{!isShowAllContent}}"><view class="text">{{items.$orig.content}}</view></block><block wx:else><view class="text">{{items.f0}}</view></block><block wx:if="{{items.$orig.content.length>45}}"><view data-event-opts="{{[['tap',[['open']]]]}}" class="anCotent" bindtap="__e">{{brandFold?'收起':'展开'}}<image class="{{[!brandFold?'':'in']}}" src="http://www.zhongjubang.com/api/upload/static/drafts/arrow-bottom.png" mode></image></view></block></view></block><view class="imageList"><block wx:for="{{items.$orig.imgList}}" wx:for-item="row" wx:for-index="indexI" wx:key="indexI"><image class="{{[items.$orig.imgList.length>1?'imageListIn':'imageListSingle']}}" mode="{{items.$orig.imgList.length>1?'aspectFit':''}}" src="{{row.fileUrl}}" data-event-opts="{{[['tap',[['previewImage',[indexI,'$0'],[[['releaseImgList','',index,'imgList']]]]]]]}}" catchtap="__e"></image></block></view><block wx:if="{{items.$orig.title.topic!=''}}"><view hidden="{{!(show)}}" class="release-image_topic"><view data-event-opts="{{[['tap',[['goTopic',['$0'],[[['releaseImgList','',index,'title']]]]]]]}}" class="left" catchtap="__e"><image src="http://www.zhongjubang.com/api/upload/static/topic/topic.png" mode></image><view>{{items.$orig.title.topic}}</view></view><view class="right"></view></view></block><view class="operate-bottom"><view class="operate-bottom_share"><image src="http://www.zhongjubang.com/api/upload/static/img/user/share.png" mode></image></view><view class="operate-bottom_number"><view data-event-opts="{{[['tap',[['togglePopup',['bottom','comments','$0','$1','$2','$3'],[[['releaseImgList','',index,'userId']],[['releaseImgList','',index,'gcircleContentId']],'nickName',[['releaseImgList','',index,'gCollectionDiscussNum']]]]]]]}}" class="number-message" catchtap="__e"><image src="http://www.zhongjubang.com/api/upload/static/img/topicDetails/message.png" mode></image><text>{{items.$orig.gCollectionDiscussNum}}</text></view><view class="collect"><image src="{{activeIndex==index&&isShowCollect||items.$orig.collectionState===1?'http://www.zhongjubang.com/api/upload/static/topic/collect-select.png':'http://www.zhongjubang.com/api/upload/static/img/user/star.png'}}" mode data-event-opts="{{[['tap',[['collect',[index,'$0','$1'],[[['releaseImgList','',index,'gcircleContentId']],[['releaseImgList','',index,'collectionState']]]]]]]}}" catchtap="__e"></image><text>{{items.$orig.collectionNum}}</text></view><view class="fabulous"><image src="{{fabulousIndex==index&&isShowFabulous||items.$orig.gcircleContentLikeState===1?'http://www.zhongjubang.com/api/upload/static/topic/fabulous-select.png':'http://www.zhongjubang.com/api/upload/static/img/user/good.png'}}" mode data-event-opts="{{[['tap',[['fabulous',[index,'$0','$1'],[[['releaseImgList','',index,'gcircleContentId']],[['releaseImgList','',index,'gcircleContentLikeState']]]]]]]}}" catchtap="__e"></image><text>{{items.$orig.gcircleContentLikeNum}}</text></view></view></view></view></view></block></view></block><view hidden="{{!(showEdit)}}" id="mask"></view><uni-popup class="comments-list vue-ref" vue-id="1" type="{{popupType}}" custom="{{true}}" data-ref="comments" data-event-opts="{{[['^change',[['popupChange']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-comments"><view class="uni-comments-title"><view>{{"全部评论("+gCollectionDiscussNum+")"}}</view><view data-event-opts="{{[['tap',[['cancelPopup',['comments']]]]]}}" catchtap="__e"><image src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/close.png" mode></image></view></view><view class="uni-comments-content"><block wx:for="{{commentItem}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="comments-detail"><view class="comments-user"><image src="{{row.head}}" mode></image><view><text class="comments-name">{{row.nick_name}}</text><text class="date">{{row.createTime}}</text></view><view data-event-opts="{{[['tap',[['commentsFabulous',[index,'$0','$1'],[[['commentItem','',index,'id']],[['commentItem','',index,'state']]]]]]]}}" class="fabulous" catchtap="__e">{{''+row.likenum+''}}<image src="{{activeIndex==index&&isCommentsFabulous||row.state!=0?'http://www.zhongjubang.com/api/upload/static/topic/fabulous-select.png':'http://www.zhongjubang.com/api/upload/static/img/user/good.png'}}" mode></image></view></view><view class="comments-content"><text data-event-opts="{{[['tap',[['testreply',['$0','$1'],[[['commentItem','',index,'id']],[['commentItem','',index,'nick_name']]]]]]]}}" bindtap="__e">{{row.gcircle_content_discuss}}</text><view hidden="{{!(row.zilist.length>0)}}" class="reply-comments"><block wx:for="{{row.zilist}}" wx:for-item="rows" wx:for-index="indexs" wx:key="indexs"><view><text data-event-opts="{{[['tap',[['replyComments',['$0','$1','$2'],[[['commentItem','',index],['zilist','',indexs,'pid']],[['commentItem','',index],['zilist','',indexs,'id']],[['commentItem','',index],['zilist','',indexs,'nick_name']]]]]]]}}" catchtap="__e">{{rows.ziNickName+'回复'+rows.outUserName+"："+rows.gcircle_content_discuss}}</text></view></block><text data-event-opts="{{[['tap',[['reply',['$0'],[[['commentItem','',index,'id']]]]]]]}}" hidden="{{!(row.sonCount>2)}}" class="all-replay" bindtap="__e">{{"共"+row.sonCount+"条回复 >"}}</text></view><text class="parting-line"></text></view></view></block></view><view class="comments-botton"><input placeholder="{{replySay}}" type="text" data-event-opts="{{[['confirm',[['recordName',['$event']]]]]}}" value="{{inputValue}}" bindconfirm="__e"/></view></view></uni-popup><block wx:if="{{showTopbar}}"><view class="topbar"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="left-menu" bindtap="__e"><image src="http://www.zhongjubang.com/api/upload/static/img/back.png" mode></image></view></view></block></view>