/* 分类start */
.G-list_content {
	margin-top: 20rpx;
    box-sizing: border-box;
    padding-left: 90px;
	height: 70rpx;
	background: #FFFFFF;
}
.G-list_content view {
	width: 36%;
	height: 34rpx;
	line-height: 34rpx;
	text-align: center;
	color: #666666;
	font-size: 28rpx;
	display: inline-block;
	/* border-right: 1rpx solid #E2E2E2; */
}
.G-list_content view:last-child {
	border: none;
}
.active {
	color: #F9B72C !important;
	border-width: 50rpx;
	border-color: red;
}
.active-status {
	margin: 9px auto;
	display: block;
	width:52rpx;
	height:6rpx;
	background:rgba(249,183,44,1);
    border-radius:4px;
}
