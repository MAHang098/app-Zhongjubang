<view><view class="navigate"><text class="navigate-image"><image style="width:15rpx;height:31rpx;" src="../../static/img/juquanVideo/back.png"></image></text><text class="car">{{"购物车("+length+")"}}</text><text class="edit">编辑</text></view><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="car-content"><view><view class="car-all-picker"></view><image class="car-all-icon" style="width:29rpx;height:28rpx;" src="{{item.shopLogo}}"></image><text class="car-title">{{item.shopName+" >"}}</text></view><block wx:if="{{item.goodsTypeId==1}}"><view class="car-all-box"><view class="car-all-command"></view><view class="car-all-image"><image style="width:150rpx;height:150rpx;" src="{{item.topImgList[0]}}"></image></view><view class="car-all-des">{{item.goodsName}}</view><text class="car-all-name">全部套装</text><text class="car-all-price">{{"￥"+item.goodsPrice}}</text><text class="car-all-count">{{"x"+item.quantity}}</text></view></block></view></block><view class="footer"><view class="picker"></view><text class="all-pick">全选</text><text class="total">合计:</text><text class="price">￥0</text><view class="balance">结算(0）</view></view></view>