<view><view class="header"><view class="search-input"><image src="../../../static/search/nav-search.png" mode></image><input type="text" value="" placeholder="搜索您需要的商品" data-event-opts="{{[['input',[['gainInput',['$event']]]],['focus',[['onFocus',['$event']]]],['blur',[['onBlur',['$event']]]]]}}" bindinput="__e" bindfocus="__e" bindblur="__e"/></view><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="cancel" bindtap="__e">取消</view></view><view class="content-body"><view hidden="{{!(isShowAdd)}}" class="tag-list"><view data-event-opts="{{[['tap',[['goBack',['tag']]]]]}}" class="add-tag" bindtap="__e"><image src="../../../static/img/release.png" mode></image><text>{{"点击添加"+searchInput}}</text></view></view><view class="product"><view class="product-tag"><block wx:for="{{tabType}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeProduct',[index]]]]]}}" class="{{[index==current?'active':'']}}" bindtap="__e">{{''+item+''}}<text class="{{[index==current?'active-status':'']}}"></text></view></block></view><view class="product-list"><block wx:for="{{productItem}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goBack',['product','$0'],[[['productItem','',index,'title']]]]]]]}}" class="list" catchtap="__e"><view><image src="../../../static/search/product.png" mode></image></view><view class="detail"><view>{{item.title}}</view><view class="price">{{item.price}}</view></view></view></block></view></view></view></view>