<view><view class="release-video"><image class="back" src="http://www.zhongjubang.com/api/upload/static/img/topicDetails/back.png" mode data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><image class="fly" src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/share.png" mode></image></view><view class="release-content"><view class="pic"><video show-center-play-btn="false" src="{{videoUrl}}"></video></view><view class="user-info"><view class="my-active-image10" style="{{'bottom:'+(height-10+'px')+';'}}"><image src="{{head}}" mode></image></view><view class="my-active-nickName10" style="{{'bottom:'+(height+3+'px')+';'}}">{{nickName}}</view><view data-event-opts="{{[['tap',[['attention',['$event']]]]]}}" class="edit-del" style="{{'bottom:'+(height+'px')+';'}}" bindtap="__e"><image src="http://www.zhongjubang.com/api/upload/static/img/topicDetails/interest.png" mode></image></view><view class="content"><block wx:if="{{!isShowAllContent}}"><view class="text">{{content2}}</view></block><block wx:else><view class="text">{{$root.f0}}</view></block><block wx:if="{{content2.length>60}}"><view data-event-opts="{{[['tap',[['open']]]]}}" class="anCotent" bindtap="__e">{{brandFold?'收起':'展开'}}<image class="{{[brandFold?'':'in']}}" src="http://www.zhongjubang.com/api/upload/static/drafts/arrow-bottom.png" mode></image></view></block></view></view></view><view class="footer"><view class="good"><image hidden="{{!(showUpImg2)}}" src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/good.png" data-event-opts="{{[['tap',[['good',['$event']]]]]}}" bindtap="__e"></image><image hidden="{{!(!showUpImg2)}}" src="http://www.zhongjubang.com/api/upload/static/topic/fabulous-select.png" data-event-opts="{{[['tap',[['good',['$event']]]]]}}" bindtap="__e"></image></view><text class="text10">{{shortVideoLikeNum}}</text><view class="star"><image hidden="{{!(showUpImg)}}" src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/star.png" mode data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"></image><image hidden="{{!(!showUpImg)}}" src="http://www.zhongjubang.com/api/upload/static/topic/collect-select.png" mode data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"></image></view><text class="text11">{{shortVideoCollectionNum}}</text><view data-event-opts="{{[['tap',[['togglePopup',['bottom','popup']]]]]}}" class="message" bindtap="__e"><image src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/message.png" mode></image></view><text class="text12">{{shortVideoDiscussNum}}</text><button class="topic" type="button" data-event-opts="{{[['tap',[['togglePopup',['bottom','popup']]]]]}}" bindtap="__e"><image class="topic-image" src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/message.png" mode></image><text class="text20">说点什么吧...</text></button><uni-popup class="vue-ref" vue-id="1" type="{{type}}" data-ref="popup" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="recommend"><view class="recommend-content"><view class="all-recommend">{{"全部评论 ("+shortVideoDiscussNum+")"}}</view><image class="close" src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/close.png" mode></image></view><block wx:for="{{dataList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="replay-detail"><view class="recommend-content2"><view class="my-active-image"><image src="{{row.head}}" mode></image></view><view class="nickName-data"><view class="my-active-nickName">{{row.nick_name}}</view><view class="my-active-data">{{row.cratee_time}}</view></view><text class="my-active-like">{{row.likenum}}</text><block wx:if="{{showUpImg3}}"><image class="recommend-good" src="http://www.zhongjubang.com/api/upload/static/img/releaseVideo2/good.png" data-event-opts="{{[['tap',[['replyLike',['$0'],[[['dataList','',index,'id']]]]]]]}}" bindtap="__e"></image></block><block wx:if="{{!showUpImg3}}"><image class="recommend-good" src="http://www.zhongjubang.com/api/upload/static/topic/fabulous-select.png" data-event-opts="{{[['tap',[['replyLike',['$0'],[[['dataList','',index,'id']]]]]]]}}" bindtap="__e"></image></block></view><view data-event-opts="{{[['tap',[['testreply',['$0','$1'],[[['dataList','',index,'id']],[['dataList','',index,'nick_name']]]]]]]}}" class="recommend-text" bindtap="__e">{{''+row.short_video_discuss+''}}</view><view hidden="{{!(row.zilist.length>0)}}" class="recommend-text-reply"><block wx:for="{{row.zilist}}" wx:for-item="rows" wx:for-index="indexs" wx:key="indexs"><view><text class="recommend-text-reply-user">{{rows.nick_name+":"}}</text><text class="recommend-text-reply-user2">{{rows.short_video_discuss}}</text></view></block><text data-event-opts="{{[['tap',[['reply',['$0','$1'],[[['dataList','',index,'id']],[['dataList','',index,'nick_name']]]]]]]}}" hidden="{{!(row.sonCount>2)}}" class="recommend-text-reply-count" bindtap="__e">{{"共"+row.sonCount+"条回复>"}}</text></view><view class="horizon2"></view></view></block><view class="replay-input"><input class="say-something" placeholder="{{replySay}}" data-event-opts="{{[['confirm',[['recordName',['$event']]]]]}}" value="{{inputValue}}" bindconfirm="__e"/></view></view></uni-popup></view></view>