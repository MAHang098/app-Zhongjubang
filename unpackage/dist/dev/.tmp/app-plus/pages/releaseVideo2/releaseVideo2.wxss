.comments-list {
	height: 100%;
	z-index: 100000;
}
.uni-comments {
	background: #fff;
	height: 500px;
	border-radius:22rpx 22rpx 0px 0px;
	position: relative;
}
.comments-detail:first-child {
	margin-top: 115rpx;
}
.comments-detail:last-child {
	margin-bottom: 300rpx;
}
.comments-detail {
	margin-bottom: 30rpx;
	width: 100%;
}
.uni-comments-title {
	height: 115rpx;
	line-height: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	box-sizing: border-box;
	padding: 41rpx 30rpx;
	border-bottom: 1px solid #E2E2E2;
	position: fixed;
	z-index: 1;
	top: 0;
	width: 100%;
	background: #FFFFFF;
	border-radius:22rpx 22rpx 0px 0px;
	font-size: 32rpx;
}
.uni-comments-title image {
	width: 25rpx;
	height: 25rpx;
	display: block;
}
.uni-comments-content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	padding: 15px;
	overflow-y: scroll;
	-webkit-overflow-scrolling: touch; 
	height: 100%;
}
.uni-comments-content-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 25%;
	box-sizing: border-box;
}
.uni-comments-content-image {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		width: 60rpx;
		height: 60rpx;
		overflow: hidden;
		border-radius: 10rpx;
}
.uni-comments-content-image .image {
	width: 100%;
	height: 100%;
}
.uni-comments-content-text {
	font-size: 26rpx;
	color: #333;
	padding-top: 5px;
	padding-bottom: 10px;
}
.uni-comments-btn {
	height: 90rpx;
	line-height: 90rpx;
	border-top: 1px #f5f5f5 solid;
	text-align: center;
	color: #666;
}
