<view class="contant"><view class="all-follow"><view class="tabList"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeProduct',[index]]]]]}}" class="{{[index==current?'active':'']}}" bindtap="__e">{{''+item+''}}<text class="{{[index==current?'active-status':'']}}"></text></view></block></view><view hidden="{{!(current==0)}}" class="follow-fans"><block wx:for="{{followData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="fans-list"><view class="left"><view class="avatar"><image src="{{item.head}}" mode></image></view><view class="details"><view class="name">{{item.nickName+''}}<image src="../../../static/fans-logo.png" mode></image></view><text class="follow">{{"粉丝"+item.fanCount}}</text></view></view><view class="right"><image src="{{item.state==0?'../../../static/follow-checked.png':'../../../static/mutual-follow.png'}}" mode data-event-opts="{{[['tap',[['followFnas',['$0','0'],[[['followData','',index,'outUserId']]]]]]]}}" catchtap="__e"></image></view></view></block></view><view hidden="{{!(current==1)}}" class="fans"><block wx:for="{{fansData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="fans-list"><view class="left"><view class="avatar"><image src="{{item.head}}" mode></image></view><view class="details"><view class="name">{{item.nickName+''}}<image src="../../../static/fans-logo.png" mode></image></view><text class="follow">{{"粉丝"+item.fanCount}}</text></view></view><view class="right"><image src="{{item.state==0?'../../../static/follow.png':'../../../static/mutual-follow.png'}}" mode data-event-opts="{{[['tap',[['followFnas',['$0','1'],[[['fansData','',index,'inUserId']]]]]]]}}" catchtap="__e"></image></view></view></block></view></view><view hidden="{{!(isShow)}}" id="mask"></view><view hidden="{{!(isShow)}}" class="bottom"><view>不在关注</view><view>取消</view></view></view>