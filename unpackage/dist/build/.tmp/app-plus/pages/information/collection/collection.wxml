<view class="collection"><block wx:for="{{allData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="collection-list"><view class="details-list"><view class="left"><view class="avatar"><image src="{{item.msg.head}}" mode></image></view><view class="details"><view class="name">{{item.msg.nickName+''}}<block wx:if="{{item.msg.title=='设计达人'}}"><image src="../../../static/img/title/design-people.png" mode></image></block><block wx:if="{{item.msg.title=='人气网红'}}"><image src="../../../static/img/title/red-hot.png" mode></image></block><block wx:if="{{item.msg.title=='居圈达人'}}"><image src="../../../static/img/title/circle-people.png" mode></image></block><block wx:if="{{item.msg.title=='金牌业主'}}"><image src="../../../static/img/title/gold-owner.png" mode></image></block></view><view class="time">{{item.createTime}}</view><view class="follow">{{item.msg.text}}</view></view></view><view class="right"><block wx:if="{{item.topImg}}"><image src="{{item.topImg}}" data-event-opts="{{[['tap',[['contentDetail',['$0','$1'],[[['allData','',index,'msg.id']],[['allData','',index,'topImg']]]]]]]}}" catchtap="__e"></image></block><block wx:else><image src="{{item.msg.imgList[0].fileUrl}}" mode data-event-opts="{{[['tap',[['contentDetail',['$0','$1'],[[['allData','',index,'msg.id']],[['allData','',index,'topImg']]]]]]]}}" catchtap="__e"></image></block></view></view></view></block></view>