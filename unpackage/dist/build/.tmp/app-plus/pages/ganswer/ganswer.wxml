<view><view class="top"><view class="release-video"><image class="back" src="../../static/img/back.png" mode data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><text class="title">{{length+"条回复"}}</text></view><view class="top-image"><image src="{{head}}" mode></image></view><view class="top-nickname">{{nickName}}</view><view class="top-date">11-18 09:12</view><view class="top-content">{{gcircleContentDiscuss}}</view><view data-event-opts="{{[['tap',[['checkContent',['$event']]]]]}}" class="top-check" bindtap="__e">查看原内容></view><image class="good" src="../../static/img/releaseVideo2/good.png" mode></image></view><view class="all-content"><block wx:for="{{dataList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="all-content-details"><view class="content-details-image"><image src="{{row.ziHead}}" mode></image></view><view class="content-details-left"><view class="content-details-nickname">{{row.ziName}}</view><view class="content-details-date">{{row.ziCreateTime}}</view><view class="content-details-content">{{row.ziContext}}</view></view><view class="content-details-good"><image src="../../static/img/releaseVideo2/good.png" mode></image><view class="like">{{row.ziLikeNum}}</view></view><view class="level"></view></view></block><view class="footter">回复晴天小猪：</view></view></view>